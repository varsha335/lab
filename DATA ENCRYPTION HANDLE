#include <stdio.h>

int main() {
    char data[] = "SECRET";
    char key = 5;
    int i;

    // Encrypt
    for(i=0; data[i]; i++) data[i] ^= key;
    printf("Encrypted: %s\n", data);

    // Decrypt
    for(i=0; data[i]; i++) data[i] ^= key;
    printf("Decrypted: %s\n", data);

    return 0;
}
DATA ENCRYPTION HANDLE
